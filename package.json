{
  "name": "gohan-jsonschema-form",
  "version": "0.0.7",
  "description": "Gohan jsonschema forms.",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "pretty-quick --staged && ./hooks/precommit.js",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "build:lib": "tsc --emitDeclarationOnly && NODE_ENV=lib babel src --out-dir lib --extensions \".ts,.tsx\"",
    "build": "rm -rf ./dist/ && NODE_ENV=production webpack -p --colors --progress --bail && npm run build:lib",
    "build-dev": "rm -rf NODE_ENV=development ./dist/ && webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && NODE_ENV=development webpack-dev-server --hot --inline --colors --progress",
    "doc": "esdoc -c .esdoc.json",
    "test": "NODE_ENV=test mocha \"src/**/*.test.{ts,tsx}\"",
    "test-watch": "npm test -- --watch",
    "test-cover": "NODE_ENV=test istanbul cover _mocha \"src/**/*.test.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json -t codeFrame -c tslint.json \"src/**/*.{ts,tsx}\" -e \"**/*.test.{ts,tsx}\"",
    "publish-to-npm": "npm run build:lib && npm publish",
    "postinstall": "npm run build:lib && cp -a ./node_modules/leaflet/dist/images ./lib"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudwan/gohan-jsonschema-form.git"
  },
