{
  "name": "gohan-jsonschema-form",
  "version": "0.0.1",
  "description": "Gohan jsonschema forms.",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "pretty-quick --staged && ./hooks/precommit.js",
    "prettier": "prettier --write \"src/**/*.{js,jsx}\"",
    "build:lib": "NODE_ENV=lib babel -d ./lib ./src/",
    "build": "rm -rf ./dist/ && NODE_ENV=production webpack -p --colors --progress --bail",
    "build-dev": "rm -rf NODE_ENV=development ./dist/ && webpack -d --colors --display-reasons --progress --bail",
    "dev-server": "rm -rf ./dist/ && NODE_ENV=development webpack-dev-server --hot --inline --colors --progress",
    "doc": "esdoc -c .esdoc.json",
    "test": "NODE_ENV=test mocha \"src/**/*.test.{js,jsx}\"",
    "test-watch": "npm test -- --watch",
    "test-cover": "NODE_ENV=test istanbul cover _mocha \"src/**/*.test.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "publish-to-npm": "npm run build:lib && npm publish"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudwan/gohan-jsonschema-form.git"
  },
  "author": {
    "name": "Kamil Stepczuk",
    "email": "k.stepczuk@hotmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cloudwan/gohan-jsonschema-form/issues"
  },
  "homepage": "https://github.com/cloudwan/gohan-jsonschema-form#readme",
  "keywords": [
    "gohan",
    "react",
    "form",
    "json-schema"
  ],
  "peerDependencies": {
    "ajv": "^6.5.0",
    "antd": "^3.5.1",
    "brace": "^0.11.0",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "react": "^16.4.0",
    "react-ace": "^6.1.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-sortable-hoc": "^0.8.0"
  },
