{
  "presets": [
    ["@babel/env", {"modules": false}],
    "@babel/typescript",
    "@babel/react"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ],
  "env": {
    "lib": {
      "presets": [
        "@babel/env"
      ],
      "plugins": [
        [
          "babel-plugin-css-modules-transform",
          {
            "extractCss": "./lib/style.css",
            "keepImport": false,
            "extensions": [".css", ".scss"],
            "processCss": "./utils/processSass.js"
          }
        ]
      ]
    }
  }
}
